{
    "liveSassCompile.settings.excludeList": [
        "**/node_modules/**",
        ".vscode/**",
        "sass/parts/**",
        "sass/*[!(style)].scss"
        //"wp-content/themes/revitta/*[!(style)].scss"
    ],
    "liveSassCompile.settings.formats":[
        // This is Default.
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "~/../"
        },
        // Compressed
        {
            "format": "compressed",
            "extensionName": ".min.css",
            "savePath": "~/../"
        }
    ]
}